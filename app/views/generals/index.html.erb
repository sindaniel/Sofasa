



<div id="page-wrapper">

  <div class="container-fluid">

    <!-- Page Heading -->
    <div class="row">
      <div class="col-lg-12">
        <h1 class="page-header">
          Dashboard <small>Statistics Overview</small>
        </h1>
        <ol class="breadcrumb">
          <li class="active">
            <i class="fa fa-dashboard"></i> Dashboard
          </li>
        </ol>
      </div>
    </div>
    <!-- /.row -->





    <% if flash[:notice] %>
        <div class="row">
          <div class="col-lg-12">
            <div class="alert alert-info alert-dismissable">
              <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
              <%= flash[:notice] %>
            </div>
          </div>
        </div>
    <% end %>

    <div class="row">

      <div class="col-lg-12">
        <div class="panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title"><i class="fa fa-car fa-fw"></i>Vehiculos</h3>
          </div>
          <div class="panel-body">

            <table class="table">
              <%  @generals.each do |item|  %>
                  <tr>
                    <td>
                      <%= item.id %>
                    </td>
                    <td>
                      <%= item.key %>
                    </td>

                    <td>
                      <%= item.value %>''
                    </td>


                    <td>
                      <%= link_to 'Editar', edit_general_path(item.id), :class=>'btn btn-info' %>

                    </td>
                  </tr>
              <% end %>
            </table>
          </div>
        </div>
      </div>

    </div>
    <!-- /.row -->

  </div>
  <!-- /.container-fluid -->

</div>
<!-- /#page-wrapper -->

